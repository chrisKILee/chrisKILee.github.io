<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
<title>Test page using Test Harness</title>
</head>
<body onload="load_test_attr.done()">
<h1>Element Traversal Specification - 2008-12-22</h1>
<div id="log"></div>
<script src="testharness.js"></script>
<script src="testharnessreport.js"></script>
<script src="loadxmldoc.js"></script>
<script>



test(function(){
        assert_equals(ElementTraversal, undefined, 'TEST01 : Interface_ElementTraversal');
			}, "TEST01 : Interface_ElementTraversal");

test(function(){
		xmlDoc = loadXmlDoc('books.xml');
	    var firstElementChild = xmlDoc.getElementsByTagName('book')[0].firstElementChild;
        assert_equals(firstElementChild.nodeName, "title", 'TEST02 : Interface_ElementTraversal_firstElementChild');
			}, "TEST02 : Interface_ElementTraversal_firstElementChild");

test(function(){
		xmlDoc = loadXmlDoc('books.xml');
		var lastElementChild = xmlDoc.getElementsByTagName('book')[0].lastElementChild;
		assert_equals( lastElementChild.nodeName , 'price', 'TEST03 : Interface_ElementTraversal_lastElementChild'); 
		}, "TEST03 : Interface_ElementTraversal_lastElementChild");

test(function(){
		xmlDoc = loadXmlDoc('books.xml');
		var previousElementSibling = xmlDoc.getElementsByTagName('book')[0].previousElementSibling;
		assert_equals( previousElementSibling , null, 'TEST04 : Interface_ElementTraversal_previousElementSibling'); 
		}, "TEST04 : Interface_ElementTraversal_previousElementSibling");

test(function(){
		xmlDoc = loadXmlDoc('books.xml');
		var nextElementSibling = xmlDoc.getElementsByTagName('book')[0].nextElementSibling;
		assert_equals( nextElementSibling.nodeName  , 'book', 'TEST05 : Interface_ElementTraversal_nextElementSibling'); 
		}, "TEST05 : Interface_ElementTraversal_nextElementSiblingg");

test(function(){
		xmlDoc = loadXmlDoc('books.xml');
		var childElementCount = xmlDoc.getElementsByTagName('book')[0].childElementCount;
		assert_equals(childElementCount  , 4, 'TEST06 : Interface_ElementTraversal_childElementCount'); 
		}, "TEST06 : Interface_ElementTraversal_childElementCount");

</script>
</body>
</html>


 
